{
    "name": "WhimsyWhispers",
    "version": "1.0.0",
    "description": "Chrome Extension for Sweet Child Name Selection Query",
    "manifest_version": 3,
    "author": "Jasper Villarosa",
    "action": {
        "default_popup": "index.html",
        "default_title": "WhimsyWhispers"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "baby.png",
        "48": "baby.png",
        "128": "baby.png"
      }
      
}